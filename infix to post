void push(char x)
{
    if(top==MAX-1)
    {
        printf("stack is overflown");
        exit(1);
    }
    top++;
    stack[top]=x;
}
char pop()
{
    if(top==-1)
    {
        printf("stock is underflown");
        exit(1);
    }
    c=stack[top];
    top--;
    return c;
}
